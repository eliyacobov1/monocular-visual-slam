{
  "run_id": "tum_freiburg1_eval",
  "dataset": "tum",
  "seed": 7,
  "output_dir": "reports/tum_freiburg1",
  "use_run_subdir": true,
  "baseline": {
    "store_path": "reports/baselines.json",
    "key": "tum_freiburg1_smoke",
    "thresholds": {
      "ATE_RMSE": { "direction": "lower", "tolerance": 0.05 },
      "RPE_RMSE": { "direction": "lower", "tolerance": 0.05 }
    },
    "telemetry": {
      "thresholds": {
        "telemetry_mean_duration_s": { "direction": "lower", "tolerance": 0.02 }
      }
    },
    "write": false
  },
  "trajectories": [
    {
      "name": "freiburg1_xyz",
      "gt_path": "data/tum/rgbd_dataset_freiburg1_xyz/groundtruth.txt",
      "est_path": "outputs/tum_freiburg1/estimated.txt",
      "format": "xy",
      "cols": "1,2",
      "est_cols": "0,1",
      "rpe_delta": 5
    }
  ]
}
